{
  "name": "vpoll-backend",
  "version": "1.0.0",
  "description": "Vpoll backend",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy-templates": "rimraf  dist/app/src/core/templates && cp -r src/core/templates dist/app/src/core/templates",
    "docker:dev": "npm install && rimraf dist && nest build && npm run copy-templates && nest start --watch",
    "migrate:fix-legacy-passwords": "ts-node -r tsconfig-paths/register scripts/fix-legacy-user-passwords.ts"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.9",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "6.4.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "7.6.18",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/websockets": "^7.6.18",
    "@typegoose/typegoose": "^7.6.0",
    "@types/babel__traverse": "^7.20.7",
    "axios": "^0.19.2",
    "bcrypt": "^5.1.1",
    "bignumber.js": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-string": "^4.0.1",
    "exceljs": "^4.3.0",
    "handlebars": "~4.7.6",
    "helmet": "^4.6.0",
    "jest-junit": "^13.0.0",
    "jsrsasign": "^10.5.26",
    "maildev": "^2.2.1",
    "moment": "^2.29.3",
    "mongo-seeding": "^3.7.0",
    "mongoose": "5.10.18",
    "mongoose-autopopulate": "^0.12.2",
    "mongoose-hidden": "^1.9.0",
    "mongoose-lean-defaults": "^0.4.1",
    "nestjs-typegoose": "^7.1.38",
    "node-xlsx": "^0.21.0",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^13.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "slugify": "^1.4.6",
    "socket.io": "^4.8.1",
    "stacktrace-parser": "^0.1.10",
    "twilio": "^3.73.1",
    "uuid": "^8.3.2",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/eslint": "^8.56.12",
    "@types/estree": "^0.0.51",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/mongoose": "5.10.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.4.0",
    "webpack": "^5.99.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "mongoose": "5.10.18"
  }
}
